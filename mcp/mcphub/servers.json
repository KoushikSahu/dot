{
  "nativeMCPServers": [],
  "mcpServers": {
    "git": {
      "args": ["mcp-server-git"],
      "command": "uvx"
    },
    "playwright": {
      "args": ["-y", "@executeautomation/playwright-mcp-server"],
      "command": "npx"
    },
    "fetch": {
      "args": ["mcp-server-fetch"],
      "command": "uvx"
    },
    "markdownify": {
      "args": ["~/.mcphub/servers/markdownify/dist/index.js"],
      "command": "node",
      "env": {
        "UV_PATH": "/usr/bin/uv"
      }
    },
    "sequentialthinking": {
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "disabled": false,
      "command": "npx"
    }
  }
}
